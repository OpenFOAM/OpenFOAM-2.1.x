FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      combustionProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

combustionModel  PaSR<rhoChemistryCombustionModel>;

active  true;

PaSRCoeffs
{
    Cmix                Cmix [ 0 0 0 0 0 0 0 ] 0.1;
    turbulentReaction   on;
    useReactionRate     true;
}


